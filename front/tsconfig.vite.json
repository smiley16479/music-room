{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2022"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true,
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"types": ["node", "vite/client"]
	},
	"include": ["vite.config.ts"]
}
